                        <nav id="pagination" class=" pagination_pages">
                      <%=link_to_unless_current("<<", searcharticles_path(s: params[:s]), {class: "pager_first"}) {''}%>

                      <%=params[:id] ? (link_to_unless((params[:id] == "2"), "<", searcharticles_path(id: (params[:id].to_i - 1), s: params[:s] ), {class: "pager_prev"}) do
                        link_to("<", searcharticles_path(s: params[:s] ), class: "pager_prev")
                      end) : nil%>


<%=render partial: "manavigationsupertop", collection: @mypages, as: :pages, locals: {pageid: (params[:id].to_i )}%>
                      <%=link_to_unless(((params[:id].to_i )>=@mypages[-1..-1].map{|a,b| a*10+b}.first ), ">", searcharticles_path(id: (params[:id].to_i + 1), s: params[:s] ), {class: "pager_next"}) {''}%>

                      <%=link_to_unless_current(">>", searcharticles_path(id: @mypages[-1..-1].map{|a,b| a*10+b}.first, s: params[:s]), {class: "pager_last"}) {''}%>

                        </nav>

